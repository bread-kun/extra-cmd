{
  "name": "extra-cmd",
  "displayName": "ExtraCmd",
  "description": "ExtraCmd is a extension for expand text editor command",
  "version": "0.0.1",
  "publisher": "asan",
  "engines": {
    "vscode": "^1.8.0"
  },
  "license": "MIT",
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.extra-cmd.deduplicate",
    "onCommand:extension.extra-cmd.transpose"
  ],
  "main": "./dist/index",
  "contributes": {
    "commands": [
      {
        "command": "extension.extra-cmd.deduplicate",
        "title": "Deduplicate"
      }, {
        "command": "extension.extra-cmd.transpose",
        "title": "Transpose"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "tsc -p ./",
    "compile": "tsc -watch -p ./",
        "build": "rm -f extra-cmd-* && vsce package -o extra-cmd-0.0.1.vsix"
  },
  "devDependencies": {
    "@types/mocha": "^9.1.1",
    "@types/node": "^17.0.42",
    "@types/vscode": "^1.8.0",
    "@vscode/test-electron": "^2.1.4",
    "@vscode/vsce": "^3.7.1",
    "typescript": "^4.7.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/asan/extra-cmd.git"
  }
}
